<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
         version="3.1">

    <!-- Servlet Configuration for ApplianceDatabaseServlet1 -->
    <servlet>
        <servlet-name>ApplianceDatabaseServlet</servlet-name>
        <servlet-class>ApplianceDatabaseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplianceDatabaseServlet</servlet-name>
        <url-pattern>/ApplianceDatabaseServlet</url-pattern>
    </servlet-mapping>

    <!-- Servlet Configuration for ApplianceDatabaseServlet2 -->
    <servlet>
        <servlet-name>ApplianceDatabaseServlet2</servlet-name>
        <servlet-class>ApplianceDatabaseServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplianceDatabaseServlet2</servlet-name>
        <url-pattern>/ApplianceDatabaseServlet2</url-pattern>
    </servlet-mapping>

    <!-- Servlet Configuration for ApplianceDatabaseServlet3 -->
    <servlet>
        <servlet-name>ApplianceDatabaseServlet3</servlet-name>
        <servlet-class>ApplianceDatabaseServlet3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplianceDatabaseServlet3</servlet-name>
        <url-pattern>/ApplianceDatabaseServlet3</url-pattern>
    </servlet-mapping>

</web-app>
